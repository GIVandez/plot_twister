│   │   │   ├──📄 pagesList.js          # js код, в котором описана логика отображения списка страниц
│   │   │   ├──📄 page.js               # js код, в котором описана логика отображения редактируемой страницы
│   │   ├── 📂 styles                   # Стили объектов на странице
│   │   │   ├──📄 pagesList.css         # css код, в котором описаны стили отображения списка страниц
│   │   │   ├──📄 page.css              # css код, в котором описаны стили отображения редактируемой страницы
│   │   └── 📄 script.html              # html код, в котором описан код страницы, на котором проходит взаимодействие со страницами сценария
│   │
│   ├── 📂 storyboard                   # Редактор раскадровки
│   │   ├── 📂 jsscripts                # js код
│   │   │   ├──📄 frame-info.js         # js код, в котором описана логика отображения информации о кадре
│   │   │   ├──📄 page-info.js          # js код, в котором описана логика отображения информации о связанной странице
│   │   │   ├──📄 frame-list.js         # js код, в котором описана логика отображения списка всех кадров в раскадровке
